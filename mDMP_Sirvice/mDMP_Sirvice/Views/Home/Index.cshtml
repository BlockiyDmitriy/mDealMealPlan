<@model IEnumerable<mDMP_Sirvice.Models.CategoryModel>

    <div>
        <h3>Распродажа книг</h3>
        <table>
            <tr class="header"><td><p>Название книги</p></td>
                <td><p>Автор</p></td>
                <td><p>Цена</p></td><td></td>
            </tr>
            @foreach (mDMP_Sirvice.Models.CategoryModel b in Model)
            {
            <tr>
                <td><p>@b.category.Name</p></td>
                <td><p>@b.category.Products</p></td>
                
            </tr>
            }
        </table>
    </div>
